{
  "LocalStack": {
    "UseLocalStack": true,
    "Session": {
      "RegionName": "eu-west-2"
    },
    "Config": {
      "LocalStackHost": "localhost",
      "EdgePort": 4566
    }
  },
  "Mongo": {
    "DatabaseUri": "mongodb://127.0.0.1:27017",
    "DatabaseName": "cdp-notify-backend"
  },
  "PagerDuty": {
    "Url": "http://localhost:3939/pagerduty"
  },
  "GrafanaAlertListener": {
    "QueueUrl": "http://localhost:4566/000000000000/cdp_grafana_alerts"
  },
  "TeamSyncer": {
    "PollIntervalSecs": 300
  },
  "EmailClient": {
    "BaseUrl": "http://localhost:3939/msgraph/v1.0/"
  },
  "DetailedErrors": true,
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u4}] ({Application}/{MachineName}/{ThreadId}/{SourceContext}.{Method}) {Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}
