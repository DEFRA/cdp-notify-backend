{
  "Mongo": {
    "DatabaseUri": "mongodb://set-automatically-when-deployed/admin?authSource=$external&authMechanism=MONGODB-AWS",
    "DatabaseName": "cdp-notify-backend"
  },
  "GrafanaAlertListener": {
    "QueueUrl": "http://localhost:4566/000000000000/cdp_notify_grafana_alerts",
    "Enabled": true
  },
  "GithubEventListener": {
    "QueueUrl": "http://localhost:4566/000000000000/cdp-notify-github-events",
    "Enabled": true
  },
  "PagerDuty": {
    "Url": "https://events.eu.pagerduty.com"
  },
  "SlackHandler": {
    "TopicArn": "arn:aws:sns:eu-west-2:000000000000:cdp-notification"
  },
  "EmailClient": {
    "SenderAddress": "CDP-Alerts@defra.gov.uk"
  },
  "PortalBackend": {
    "Url": "http://localhost:5094"
  },
  "TeamSyncer": {
    "Scheduler": {
      "quartz.scheduler.instanceName": "TeamSyncer Scheduler for fetching entities",
      "quartz.threadPool.maxConcurrency": 1
    },
    "PollIntervalSecs": 900
  },
  "AllowedHosts": "*",
  "TraceHeader": "x-cdp-request-id",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog"
        }
      }
    ]
  }
}
